            <type>string</type>
            <default>general_tickmark_unchecked</default>
            <locale name="C">
                <short>Connecting statusbar icon name for Dummy provider</short>
                <long>Connecting statusbar icon name for the Dummy provider ICD2 plugin</long>
            </locale>
        </schema>
        <schema>
            <key>/schemas/system/osso/connectivity/srv_provider/DUMMY/default_custom_ui/markup</key>
            <applyto>/system/osso/connectivity/srv_provider/DUMMY/default_custom_ui/markup</applyto>
            <owner>libicd_provider_dummy</owner>
            <type>string</type>
            <default>%s &lt;b&gt;(Dummy)&lt;/b&gt;</default>
