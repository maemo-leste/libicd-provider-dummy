                <short>Icon name for Dummy provider</short>
                <long>Icon name for the Dummy provider ICD2 plugin</long>
            </locale>
        </schema>
        <schema>
            <key>/schemas/system/osso/connectivity/srv_provider/DUMMY/default_custom_ui/type_statusbar_icon_name</key>
            <applyto>/system/osso/connectivity/srv_provider/DUMMY/default_custom_ui/type_statusbar_icon_name</applyto>
